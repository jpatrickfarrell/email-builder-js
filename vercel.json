{
  "version": 2,
  "builds": [
    {
      "src": "packages/editor-sample/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "packages/editor-sample/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/packages/editor-sample/dist/$1"
    }
  ],
  "buildCommand": "cd packages/editor-sample && npm run build",
  "outputDirectory": "packages/editor-sample/dist",
  "installCommand": "npm ci && cd packages/editor-sample && npm ci"
}
